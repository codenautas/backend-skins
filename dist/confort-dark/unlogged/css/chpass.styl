body
  background-color #363947
  
.chpasstable
  background-color #62667a
  max-width 450px
  min-width 400px
  margin-left auto
  margin-top 30px
  margin-right auto
  border 1px solid #000000
  
.formtd
  width 90%
  
.iconostd
  width 10% 
  
.formtitle
  color #3f4352
  background #535387
  background -webkit-linear-gradient(#8ce8f4, #77b1bd)
  background -o-linear-gradient(#8ce8f4, #77b1bd)
  background -moz-linear-gradient(#8ce8f4, #77b1bd)
  background linear-gradient(#8ce8f4, #77b1bd)
  border solid 2px #555866
  text-align center
  padding 10px
  
h2
  color white
  font-family helvetica
  text-transform uppercase
  font-size 80%
  font-weight 700
  
.formImg
  padding 0px
  width 210px
  display block
  margin-left auto
  margin-right auto

.chpasstableRightImg
  display none  

.formlabel
  display none
  
.iconoslog
  width 37px
  padding 5px
  margin-top 5px
  margin-left 20px

.iconosnewPassword
  width 37px
  padding 5px
  margin-top 5px
  margin-left 20px

.iconosrepPassword
  width 37px
  padding 5px
  margin-top 5px
  margin-left 20px
  
input[type="text"]
  -webkit-autofill
  background-color #79819c
  border 1px solid #000000
  -webkit-box-shadow 0 0 0px 1000px #79819c inset
  -webkit-text-fill-color #273966
  padding 10px
  width 80%     
  
input[type="text"]:focus
  -webkit-autofill
  background-color #FFF
  border 1px solid #62d3e2
  -webkit-box-shadow 0 0 0px 1000px #FFF inset
  -webkit-text-fill-color #273966

input[type=password]
  -webkit-autofill
  background-color #79819c
  border 1px solid #000000
  -webkit-box-shadow 0 0 0px 1000px #79819c inset
  -webkit-text-fill-color #273966
  padding 10px
  width 80%
  
input[type=password]:focus
  -webkit-autofill
  background-color #FFF
  border 1px solid #62d3e2
  -webkit-box-shadow 0 0 0px 1000px #FFF inset
  -webkit-text-fill-color #273966
  
input[type=submit]
  background #8de9f5
  background -webkit-linear-gradient(#8de9f5, #5f7982)
  background -o-linear-gradient(#8de9f5, #5f7982)
  background -moz-linear-gradient(#8de9f5, #5f7982)
  background linear-gradient(#8de9f5, #5f7982)
  border-right  1px solid #535767
  border-bottom 1px solid #535767
  border-left  1px solid #535767
  border-top 1px solid #535767
  color #FFF
  font-weight 700
  width 86%
  padding 15px
  margin-top 5px
  margin-bottom 5px
  
input[type=submit]:hover
  background #8de9f5
  background -webkit-linear-gradient(#48656f, #6facb4)
  background -o-linear-gradient(#48656f, #6facb4)
  background -moz-linear-gradient(#48656f, #6facb4)
  background linear-gradient(#48656f, #6facb4)
  border-right  1px solid #535767
  border-bottom 1px solid #535767
  border-left  1px solid #535767
  border-top 1px solid #535767
  cursor pointer
  
input[type=submit]:active
  background #375560
  background -webkit-linear-gradient(#375560, #44909a)
  background -o-linear-gradient(#375560, #44909a)
  background -moz-linear-gradient(#375560, #44909a)
  background linear-gradient(#375560, #44909a)
  border-right  1px solid #535767
  border-bottom 1px solid #535767
  border-left  1px solid #535767
  border-top 1px solid #535767
  cursor pointer