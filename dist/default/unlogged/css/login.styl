body
  background-color #363947
  
.logintable
  background-color #fbfbfb
  display block
  max-width 450px
  min-width 400px
  margin-left auto
  margin-top 30px
  margin-right auto
  border 1px solid #000000
  

.formtd
  width 90%
  
.iconostd
  width 10% 
  
.formtitle
  color #d3dddc
  background #456b71
  border solid 1px #555866
  text-align center
  padding 5px
  
h2
  color #3f4352
  font-family helvetica
  text-transform uppercase
  font-size 80%
  font-weight 700
  
.formImg
  padding 30px
  width 150px
  display block
  margin-left auto
  margin-right auto

.logintableRightImg
  display none  
  
.formlabel
  display none
  
.buttonmargin
  display none
  
.iconoslog
  width 37px
  padding 5px
  margin-top 5px
  margin-left 20px
  
input[type="text"]
  -webkit-autofill
  background-color #f7c99b
  border 1px solid #000000
  -webkit-box-shadow 0 0 0px 1000px #a6a6b6 inset
  -webkit-text-fill-color #626281
  padding 10px
  width 80%   

input[type="text"]:focus
  -webkit-autofill
  background-color #FFF
  border 1px solid #62d3e2
  -webkit-box-shadow 0 0 0px 1000px #FFF inset
  -webkit-text-fill-color #444444
  
  
input[type=password]
  -webkit-autofill
  background-color #f7c99b
  border 1px solid #000000
  -webkit-box-shadow 0 0 0px 1000px #d4cdc8 inset
  -webkit-text-fill-color #444444
  padding 10px
  width 80%  

input[type=password]:focus
  -webkit-autofill
  background-color #FFF
  border 1px solid #62d3e2
  -webkit-box-shadow 0 0 0px 1000px #FFF inset
  -webkit-text-fill-color #626281
  
.submit
  background #17ccba
  border-right  1px solid #535767
  border-bottom 1px solid #535767
  border-left  1px solid #535767
  border-top 1px solid #535767
  color #444444
  font-weight 700
  width 100%
  padding 15px
  margin-top 5px
  margin-bottom 5px
  
.submit:hover
  background #f4e4bd
  font-weight 700
  cursor pointer
  
.submit:active
  background #f4e4bd
  cursor pointer