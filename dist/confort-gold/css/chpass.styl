body
  background-color #363947
  
.chpasstable
  background-color #d1cac2
  display block
  max-width 450px
  min-width 400px
  margin-left auto
  margin-top 30px
  margin-right auto
  border 1px solid #000000
  
.formtd
  width 90%
  
.iconostd
  width 10% 
  
.formtitle
  color #3f4352
  background #b08657
  background -webkit-linear-gradient(#d09f67, #b08657)
  background -o-linear-gradient(#d09f67, #b08657)
  background -moz-linear-gradient(#d09f67, #b08657)
  background linear-gradient(#d09f67, #b08657)
  border solid 1px #555866
  text-align center
  padding 5px
  
h2
  color white
  font-family helvetica
  text-transform uppercase
  font-size 80%
  font-weight 700
  
.formImg
  padding 0px
  width 110px
  display block
  margin-left auto
  margin-right auto

.chpasstableRightImg
  display none  

.formlabel
  display none
  
.iconoslog
  width 37px
  padding 5px
  margin-top 5px
  margin-left 20px

.iconosnewPassword
  width 37px
  padding 5px
  margin-top 5px
  margin-left 20px

.iconosrepPassword
  width 37px
  padding 5px
  margin-top 5px
  margin-left 20px
  
input[type="text"]
  -webkit-autofill
  background-color #f7c99b
  border 1px solid #000000
  -webkit-box-shadow 0 0 0px 1000px #a6a6b6 inset
  -webkit-text-fill-color #626281
  padding 10px
  width 100%       
  
input[type="text"]:focus
  -webkit-autofill
  background-color #FFF
  border 1px solid #62d3e2
  -webkit-box-shadow 0 0 0px 1000px #FFF inset
  -webkit-text-fill-color #626281

input[type=password]
  -webkit-autofill
  background-color #f7c99b
  border 1px solid #000000
  -webkit-box-shadow 0 0 0px 1000px #a6a6b6 inset
  -webkit-text-fill-color #626281
  padding 10px
  width 80%  
  
input[type=password]:focus
  -webkit-autofill
  background-color #FFF
  border 1px solid #62d3e2
  -webkit-box-shadow 0 0 0px 1000px #FFF inset
  -webkit-text-fill-color #626281
  
input[type=submit]
  background #1f2541
  background -webkit-linear-gradient(#1f2541, #6d6d7c)
  background -o-linear-gradient(#1f2541, #6d6d7c)
  background -moz-linear-gradient(#1f2541, #6d6d7c)
  background linear-gradient(#1f2541, #6d6d7c)
  border-right  1px solid #535767
  border-bottom 1px solid #535767
  border-left  1px solid #535767
  border-top 1px solid #535767
  color #FFF
  font-weight 700
  width 86%
  padding 15px
  margin-top 5px
  margin-bottom 5px
  
input[type=submit]:hover
  background #ddb78c
  background -webkit-linear-gradient(#ddb78c, #c89862)
  background -o-linear-gradient(#ddb78c, #c89862)
  background -moz-linear-gradient(#ddb78c, #c89862)
  background linear-gradient(#ddb78c, #c89862)
  color #FFF
  font-weight 700
  cursor pointer
  
input[type=submit]:active
  background #375560
  background -webkit-linear-gradient(#375560, #44909a)
  background -o-linear-gradient(#375560, #44909a)
  background -moz-linear-gradient(#375560, #44909a)
  background linear-gradient(#375560, #44909a)
  border-right  1px solid #535767
  border-bottom 1px solid #535767
  border-left  1px solid #535767
  border-top 1px solid #535767
  color #FFF
  font-weight 700
  width 100%
  padding 15px
  margin-top 5px
  margin-bottom 5px
  cursor pointer