body
  background-color #D8DBE5
  
.logintable
  background-color #F6F7FB
  max-width 450px
  min-width 400px
  margin-left auto
  margin-top 30px
  margin-right auto
  border 1px solid #b6bcce

.logintableRightImg
  display none  

.formtd
  width 90%
  
.iconostd
  width 10% 
  
.formlabel
  display none
  
.buttonmargin
  display none
  
.formtitle
  background #535387
  background -webkit-linear-gradient(#535387, #7e6ea1)
  background -o-linear-gradient(#535387, #7e6ea1)
  background -moz-linear-gradient(#535387, #7e6ea1)
  background linear-gradient(#535387, #7e6ea1)
  text-align center
  padding 10px
  
h2
  color white
  font-family helvetica
  text-transform uppercase
  font-size 80%
  font-weight 700
  
.formImg
  padding 30px
  width 210px
  display block
  margin-left auto
  margin-right auto

.iconoslog
  width 37px
  padding 5px
  margin-top 5px
  margin-left 20px
  
input[type="text"]
  -webkit-autofill
  background-color white
  border 1px solid #c2cde2
  -webkit-box-shadow 0 0 0px 1000px white inset
  -webkit-text-fill-color #273966
  background-color #fff
  padding 10px
  width 80%   

input[type=password]
  -webkit-autofill
  background-color white
  border 1px solid #c2cde2
  -webkit-box-shadow 0 0 0px 1000px white inset
  -webkit-text-fill-color #273966
  background-color #fff
  padding 10px
  width 80%  

  
.submit
  background #8080a3
  background -webkit-linear-gradient(#8080a3, #9898c4)
  background -o-linear-gradient(#8080a3, #9898c4)
  background -moz-linear-gradient(#8080a3, #9898c4)
  background linear-gradient(#8080a3, #9898c4)
  border-right  2px solid #7573a5
  border-bottom 2px solid #7573a5
  border-left  2px solid #7573a5
  border-top 2px solid #7573a5
  color #FFF
  font-weight 700
  width 100%
  padding 15px
  margin-top 5px
  margin-bottom 5px
  
.submit:hover
  background #7372a1
  background -webkit-linear-gradient(#585789, #799dae)
  background -o-linear-gradient(#585789, #799dae)
  background -moz-linear-gradient(#585789, #799dae)
  background linear-gradient(#585789, #799dae)
  border-right  2px solid #646791
  border-bottom 2px solid #646791
  border-left  2px solid #646791
  border-top 2px solid #646791
  color #FFF
  font-weight 700
  width 100%
  padding 15px
  margin-top 5px
  margin-bottom 5px
  cursor pointer